{
  "name": "qwik-select",
  "version": "0.0.0-semantic-release",
  "description": "A select/autocomplete component for Qwik apps.",
  "homepage": "https://phongnn.github.io/qwik-select",
  "repository": {
    "url": "https://github.com/phongnn/qwik-select.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/index.mjs",
  "qwik": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/index.mjs",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "node --inspect node_modules/vite/bin/vite.js --config app/vite.config.ts --mode ssr",
    "test": "vitest",
    "test.ci": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "cy": "cypress open",
    "e2e": "start-server-and-test dev http://localhost:5173 \"npx cypress run\"",
    "typecheck": "tsc --incremental --noEmit",
    "build": "pnpm build.lib && pnpm build.types && pnpm build.styles",
    "build.lib": "vite build --mode lib",
    "build.types": "tsc --emitDeclarationOnly",
    "build.styles": "cp src/style.css dist",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "prepare": "npx husky install",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@builder.io/qwik": "^0.11.1"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.10",
    "@builder.io/qwik-city": "0.0.112",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@faker-js/faker": "^7.6.0",
    "@testing-library/cypress": "^9.0.0",
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitest/coverage-c8": "^0.26.3",
    "cp": "^0.2.0",
    "cypress": "^12.2.0",
    "eslint": "^8.31.0",
    "eslint-plugin-qwik": "^0.16.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "node-fetch": "^3.3.0",
    "prettier": "^2.8.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "^4.9.4",
    "vite": "3.1.8",
    "vite-tsconfig-paths": "3.5.1",
    "vitest": "^0.26.3"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": []
  }
}
